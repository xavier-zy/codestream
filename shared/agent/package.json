{
	"name": "codestream-lsp-agent",
	"description": "The knowledge base behind your codebase",
	"version": "0.0.2",
	"author": "CodeStream",
	"publisher": "CodeStream",
	"private": true,
	"license": "UNLICENSED",
	"homepage": "https://codestream.com",
	"bugs": {
		"url": "https://github.com/TeamCodeStream/codestream-lsp-agent/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/TeamCodeStream/codestream-lsp-agent"
	},
	"engines": {
		"node": ">=10.2.0"
	},
	"main": "./dist/agent",
	"scripts": {
		"build": "webpack --env development=true",
		"bundle": "npm install --no-save && webpack --env production=true",
		"bundle:ci": "npm run clean:ci && npm run bundle",
		"clean": "rimraf src/protocol/*.js",
		"clean:ci": "git clean -xdf",
		"lint": "tslint --project tsconfig.json",
		"pkg": "npm run pkg-vs & npm run pkg-jetbrains",
		"pkg-jetbrains": "pkg dist/agent.js --targets node16-linux-x64,node16-macos-x64,node16-win-x64 --options max_old_space_size=2048 --out-path dist/",
		"pkg-jetbrains-mac": "pkg dist/agent.js --targets node16-macos-x64 --options max_old_space_size=2048 --output dist/agent-macos-x64",
		"pkg-vs": "pkg dist/agent-pkg.js --targets node16-win-x64 --options max_old_space_size=2048 --output dist/agent.exe",
		"pretty": "prettier --config .prettierrc --write \"./src/**/*.ts\" ./package.json ./.prettierrc",
		"rebuild": "npm run clean && npm install --no-save && npm run build",
		"update:emoji": "pushd emoji && node ./shortcodeToEmoji.js && popd",
		"watch": "webpack --watch --env development=true",
		"test:ci": "npm run test-unit",
		"test-acceptance": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha -r ts-node/register -r esm --recursive \"test/acceptance/**/*.spec.ts\"",
		"test-acceptance-ci": "npm run test-acceptance -- --reporter mocha-teamcity-reporter",
		"test-apiserver": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha -r ts-node/register -r esm --recursive \"test/api_server/**/*.spec.ts\"",
		"test-integration": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha --require ts-node/register --recursive \"test/integration/**/*.spec.ts\"",
		"test-integration-ci": "npm run test-integration -- --reporter mocha-teamcity-reporter",
		"test-unit": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mochapack 'test/unit/**/*.spec.ts' -- --reporter mocha-teamcity-reporter",
		"test-unit-watch": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mochapack --watch 'test/unit/**/*.spec.ts' -- --reporter mocha-teamcity-reporter",
		"test-unit-with-coverage": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" nyc --reporter=text --reporter=teamcity ./node_modules/.bin/mochapack 'test/unit/**/*.spec.ts' -- --reporter mocha-teamcity-reporter"
	},
	"dependencies": {
		"@grpc/grpc-js": "1.3.7",
		"@grpc/proto-loader": "0.6.4",
		"@microsoft/microsoft-graph-client": "2.0.0",
		"@newrelic/native-metrics": "8.0.0",
		"@sentry/node": "5.9.0",
		"@slack/web-api": "6.7.0",
		"@types/diff-match-patch": "1.0.32",
		"@types/lz-string": "1.3.34",
		"@types/newrelic": "7.0.3",
		"@types/semver": "7.3.9",
		"abort-controller": "3.0.0",
		"analytics-node": "3.5.0",
		"bufferutil": "4.0.6",
		"chokidar": "3.5.3",
		"date-fns": "1.30.1",
		"diff": "5.0.0",
		"diff-match-patch": "1.0.5",
		"encoding": "0.1.13",
		"eol": "0.9.1",
		"form-data": "3.0.0",
		"glob-promise": "4.2.2",
		"graphql": "15.5.0",
		"graphql-request": "4.0.0",
		"ignore": "3.3.10",
		"lodash-es": "4.17.21",
		"lz-string": "1.4.4",
		"newrelic": "8.9.1",
		"node-fetch": "2.6.7",
		"oauth": "0.9.15",
		"object-sizeof": "1.5.3",
