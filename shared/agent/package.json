		"pubnub": "4.37.0",
		"semver": "7.3.5",
		"socketcluster-client": "16.0.1",
		"stacktrace-parser": "0.1.10",
		"string-similarity": "3.0.0",
		"timed-cache": "1.1.5",
		"tiny-async-pool": "1.2.0",
		"tmp": "0.1.0",
		"utf-8-validate": "5.0.8",
